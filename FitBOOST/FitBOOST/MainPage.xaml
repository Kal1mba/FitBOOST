<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FitBOOST.MainPage"
            xmlns:local="clr-namespace:FitBOOST"
            Title="Main Page">

    <FlyoutPage.Flyout>
        <local:FlyoutMenuPage x:Name="flyoutMenuPage"/>
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:HomePage/>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
    
</FlyoutPage>
